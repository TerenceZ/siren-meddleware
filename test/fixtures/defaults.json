{
    "favicon": {
        "enabled": true,
        "priority": 10,
        "module": {
            "name": "koa-favicon",
            "arguments": [ "path:./fixtures/favicon.ico" ]
        }
    },

    "static": {
        "enabled": true,
        "priority": 30,
        "module": {
            "name": "koa-static",
            "arguments": [ "path:./.build" ]
        }
    },

    "logger": {
        "enabled": true,
        "priority": 50,
        "module": {
            "name": "koa-logger"
        }
    },

    "body": {
        "enabled": true,
        "priority": 60,
        "module": {
            "name": "koa-body-parser"
        }
    }
}
